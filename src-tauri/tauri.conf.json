{
  "build": {
    "distDir": "../public",
    "devPath": "http://localhost:5000",
    "beforeDevCommand": "",
    "beforeBuildCommand": "yarn build"
  },
  "ctx": {},
  "tauri": {
    "cli": {
      "args": [
        {
          "name": "source",
          "short": "s",
          "takesValue": true,
          "description": "html source of li elements for webmenu"
        },
        {
          "name": "placeholder",
          "short": "p",
          "description": "placeholder for fzf",
          "takesValue": true
        },
        {
          "name": "windowid",
          "short": "w",
          "description": "the x11 window id to parent this to",
          "takesValue": true
        }
      ]
    },
    "embeddedServer": {
      "active": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false
      },
      "exceptionDomain": ""
    },
    "allowlist": {
      "all": true
    },
    "window": {
      "title": "Webmenu",
      "resizable": false,
      "fullscreen": false,
      "width": 800,
      "height": 600
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "inliner": {
      "active": true
    }
  }
}
