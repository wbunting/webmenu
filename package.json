{
  "name": "webmenu",
  "version": "0.1.3",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "tauri": "tauri",
    "dev:tauri": "wait-port localhost:5000 && yarn tauri dev",
    "dev": "concurrently -k --raw -c bgRed.white,bgCyan.black -n svelte,tauri \"yarn dev:svelte\" \"yarn dev:tauri\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^10.2.3",
    "concurrently": "^5.3.0",
    "postcss-nesting": "^7.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "semantic-release": "^17.3.7",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "wait-port": "^0.2.9"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "sirv-cli": "^1.0.0",
    "tauri": "^0.14.1"
  }
}
